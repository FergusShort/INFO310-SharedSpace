<div id="event">
    <form method="POST" action="/calendar/<%= event.id %>/deleteevent">
        <button class="editbutton" id="deletebutton" type="submit">🗑</button>
    </form>
    <button class="editbutton">✎</button>
    <p>Name: <%- event.title %></p>
    <p>Description:</p>
    <p><%- event.descr %></p>
    <p>Start: <%- event.start %></p>
    <p>End: <%- event.end %></p>
</div>